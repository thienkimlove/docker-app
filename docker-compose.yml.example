web:  
  image: thienkimlove/docker-app
  volumes:
    - .:/srv/www
  links:
    - db
  environment:
     VIRTUAL_HOST: doctrine.tuelinh.vn
db:  
  image: mysql:5.6
  environment:
    MYSQL_ROOT_PASSWORD: tieungao
    MYSQL_DATABASE: dockerapp
    MYSQL_USER: dockerapp
    MYSQL_PASSWORD: tieungao
  volumes_from: 
     - data
data:
 image: mysql:5.6
 command: /bin/true
 volumes:
   - /var/lib/mysql 

